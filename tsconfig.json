{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "esnext",
        "target": "ESNext",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "paths": {
          "@/*": [
            "./src/*"
          ]
        },
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
    },
  }
  